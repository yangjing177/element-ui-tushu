                    <%--}--%>
                    <%--$("#u").append(tr);--%>
                <%--}--%>
            <%--});--%>

        <%--});--%>


    <%--</script>--%>

<%--</head>--%>

<%--<body>--%>
<%--<table>--%>
    <%--<thead>--%>
    <%--<tr>--%>
        <%--<td>编号</td>--%>
        <%--<td>书名</td>--%>
        <%--<td>作者</td>--%>
        <%--<td>时间</td>--%>
        <%--<td>图片</td>--%>
        <%--<td>操作</td>--%>
    <%--</tr>--%>
    <%--</thead>--%>
    <%--<tbody id="u"></tbody><td></td>--%>
<%--</table>--%>
<%--</body>--%>
<%--</html>--%>
